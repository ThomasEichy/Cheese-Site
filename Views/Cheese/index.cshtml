@{


}

<h1>My Cheese</h1>

<p><a asp-controller="Cheese" asp-action="Add">Add Cheese</a></p>

@if (ViewBag.cheeses.Count == 0)
{
    <p>No cheeses yet!</p>
}

<ul>
    @foreach(CheeseMVC.Models.Cheese cheese in ViewBag.cheeses)
    {
        <li> 
            Cheese: <br>
            @cheese.Name <br>
            Description: <br>
            @cheese.Description
        </li>
    }
</ul>
